{
  "name": "nx-angular-19-common-engine",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "apps/nx-angular-19-common-engine/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "options": {
        "outputPath": "dist/apps/nx-angular-19-common-engine",
        "index": "apps/nx-angular-19-common-engine/src/index.html",
        "browser": "apps/nx-angular-19-common-engine/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/nx-angular-19-common-engine/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/nx-angular-19-common-engine/public"
          }
        ],
        "styles": ["apps/nx-angular-19-common-engine/src/styles.css"],
        "scripts": [],
        "server": "apps/nx-angular-19-common-engine/src/main.server.ts",
        "prerender": true,
        "ssr": {
          "entry": "apps/nx-angular-19-common-engine/src/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kB",
              "maximumError": "1MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kB",
              "maximumError": "8kB"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "nx-angular-19-common-engine:build:production"
        },
        "development": {
          "buildTarget": "nx-angular-19-common-engine:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n"
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": ["zone.js", "zone.js/testing"],
        "tsConfig": "apps/nx-angular-19-common-engine/tsconfig.spec.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/nx-angular-19-common-engine/public"
          }
        ],
        "styles": ["apps/nx-angular-19-common-engine/src/styles.css"],
        "scripts": []
      }
    }
  }
}
